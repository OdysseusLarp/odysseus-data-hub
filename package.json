{
	"name": "odysseus-social-hub",
	"version": "0.0.0",
	"scripts": {
		"ng": "ng",
		"start": "npm run apigen && NODE_OPTIONS=--openssl-legacy-provider ng serve",
		"apigen": "openapi -s http://localhost:8888/api-docs.json -o ./src/app/api -l ts --semicolon",
		"apigen:dev": "openapi -s https://apps.odysseuslarp.dev/api-docs.json -o ./src/app/api -l ts --semicolon",
		"apigen:prod": "openapi -s http://odyserver/api-docs.json -o ./src/app/api -l ts --semicolon",
		"build": "NODE_OPTIONS=--openssl-legacy-provider ng build --base-href /social/ --prod",
		"precommit": "lint-staged",
		"test": "ng test",
		"lint": "ng lint",
		"e2e": "ng e2e"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^7.2.0",
		"@angular/cdk": "^7.3.3",
		"@angular/common": "~7.2.0",
		"@angular/compiler": "~7.2.0",
		"@angular/core": "~7.2.0",
		"@angular/forms": "~7.2.0",
		"@angular/material": "^7.2.1",
		"@angular/platform-browser": "~7.2.0",
		"@angular/platform-browser-dynamic": "~7.2.0",
		"@angular/router": "~7.2.0",
		"@ng-select/ng-select": "^2.16.4",
		"@swimlane/ngx-datatable": "^15.0.1",
		"@types/socket.io-client": "^1.4.32",
		"core-decorators": "0.19.0",
		"core-js": "^2.6.2",
		"jssip": "^3.3.6",
		"lodash": "^4.17.11",
		"moment": "^2.23.0",
		"ng-click-outside": "^4.0.0",
		"ng2-trim-directive": "^2.3.2",
		"ngx-auto-scroll": "^1.1.0",
		"ngx-markdown": "^7.1.5",
		"ngx-scrollbar": "^4.1.1",
		"ol": "^5.3.3",
		"pluralize": "^7.0.0",
		"rxjs": "~6.3.3",
		"socket.io-client": "^2.2.0",
		"tslib": "^1.9.0",
		"zone.js": "~0.8.26"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "~0.12.1",
		"@angular/cli": "~7.2.1",
		"@angular/compiler-cli": "~7.2.0",
		"@angular/language-service": "~7.2.0",
		"@types/jasmine": "~3.3.5",
		"@types/jasminewd2": "~2.0.3",
		"@types/node": "^10.12.18",
		"@types/openlayers": "^4.6.17",
		"codelyzer": "~4.5.0",
		"husky": "^1.3.1",
		"jasmine-core": "~3.3.0",
		"jasmine-spec-reporter": "~4.2.1",
		"karma": "~3.1.4",
		"karma-chrome-launcher": "~2.2.0",
		"karma-coverage-istanbul-reporter": "~2.0.1",
		"karma-jasmine": "~2.0.1",
		"karma-jasmine-html-reporter": "^1.4.0",
		"lint-staged": "^8.1.0",
		"openapi-client": "^1.0.4",
		"prettier": "^1.15.3",
		"protractor": "~5.4.2",
		"sass": "^1.58.3",
		"ts-node": "~7.0.0",
		"tslint": "~5.12.0",
		"typescript": "~3.2.2"
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm run precommit"
		}
	},
	"lint-staged": {
		"*.{js,ts,json,scss,html,md}": [
			"prettier --write",
			"git add"
		]
	}
}
